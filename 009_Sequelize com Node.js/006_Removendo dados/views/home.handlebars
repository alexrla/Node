<h1>Usu√°rios cadastrados</h1>

{{#each users}}
    <div class="item">
        <h3>{{this.id}} - {{this.name}}</h3>
        <p>{{this.occupation}}</p>
        <div class="actions">
            <a href="/users/{{this.id}}" class="details">Ver detalhes</a>
            <form action="/users/delete/{{this.id}}" method="POST">
                <input type="submit" value="Deletar" class="delete">
            </form>
        </div>
    </div>
{{/each}}